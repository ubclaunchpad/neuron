{
  "name": "neuron",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev:up": "docker compose --env-file .env -f docker-compose.dev.yaml up -d",
    "dev:down": "docker compose -f docker-compose.dev.yaml down",
    "dev:redis:clear": "docker compose -f docker-compose.dev.yaml exec redis redis-cli FLUSHALL",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:db:create": "docker compose -f docker-compose.dev.yaml exec postgres psql -U postgres -c 'CREATE DATABASE neuron_test;' || true",
    "test:db:push": "dotenv -e .env.test -- drizzle-kit push"
  },
